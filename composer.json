{
    "name": "akeneo/excel-connector-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Excel connector bundle",
    "keywords": ["pim", "akeneo", "ecommerce", "e-commerce", "excel", "connector"],
    "homepage": "http://akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Antoine Guigan",
            "email": "antoine@akeneo.com",
            "homepage": "http://akeneo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/spreadsheet-reader.git"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "akeneo/pim-community-dev": "dev-maintenance/1.0_backports",
        "phpoffice/phpexcel": "1.8.0rc1",
        "akeneo/spreadsheet-reader" : "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\ExcelConnectorBundle\\": "" }
    },
    "target-dir": "Pim/Bundle/ExcelConnectorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
